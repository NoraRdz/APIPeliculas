{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include":["./src"],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./node_modules/clean-jsdoc-theme",
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true
    },
    "clean-jsdoc-theme": {
        "theme": "dark",
        "title": "API peliculas plataforma documentacion",
        "logo": "assets/logo.png",
        "favicon": "assets/favicon.ico"
    }
}